        <activity
            android:name=".GalleryAct"
            android:label="@string/title_activity_gallery" />
        <activity
            android:name=".Profile"
            android:label="Profile" />
        <activity
            android:name=".Blogs"
            android:label="@string/title_activity_blogs" />
